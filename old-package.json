{
  "name": "axentax-compiler",
  "version": "1.0.1",
  "description": "Compile Axentax syntax in JavaScript and generate MIDI output for guitar phrasing.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/conductor.js",
  "browser": "dist/conductor.js",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "package.json"
  ],
  "keywords": [
    "guitar",
    "compiler",
    "dtm"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/axentax/axentax-compiler"
  },
  "homepage": "https://axentax.github.io/",
  "bugs": {
    "url": "https://github.com/axentax/axentax-compiler/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint": "npx eslint src/ --ext .ts",
    "dev": "npx ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "rm": "rimraf dist && rimraf ./vite-compiler/dist && rimraf ./vite-compiler/lib/conductor",
    "cp": "cp -r src/conductor vite-compiler/lib",
    "vite-build": "cd vite-compiler && npm i && npm run build && cd ../",
    "NoUse_cp-js": "cp vite-compiler/dist/conductor.umd.cjs dist/",
    "cp-js": "cp vite-compiler/dist/conductor.js dist/",
    "comments-delete": "chmod +x comments-delete.sh && ./comments-delete.sh",
    "build": "npm run rm && npm run cp && npm run vite-build && npm run cp-js && npm run comments-delete"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "eslint": "^7.8.1",
    "jest": "^30.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5"
  },
  "dependencies": {
    "@tonaljs/chord": "^5.0.0",
    "@tonejs/midi": "^2.0.28",
    "decimal.js": "^10.4.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1"
  }
}